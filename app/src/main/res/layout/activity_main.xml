            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_ref_no"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_horizontal">

                <TextView
                    android:id="@+id/tvRefNo"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:minWidth="150dp"
                    android:text="Reference No"/>

                <EditText
                    android:id="@+id/edtRefNo"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:inputType="text"
                    android:minWidth="200dp"
                    android:text=""/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_amount_auth"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_horizontal">

                <TextView
                    android:id="@+id/tvAmtAuth"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:minWidth="150dp"
                    android:text="Amount, Authorised"/>

                <EditText
                    android:id="@+id/edtAmtAuth"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:inputType="number"
                    android:minWidth="200dp"
                    android:text="1"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_trx_id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_horizontal">

                <TextView
                    android:id="@+id/tvTrxID"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:minWidth="150dp"
                    android:text="Transaction ID"/>

                <EditText
                    android:id="@+id/edtTrxID"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:inputType="number"
                    android:minWidth="200dp"
                    android:text=""/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="10dp">

                <TextView
                    android:id="@+id/tvLogArea"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:scrollbars="vertical"
                    android:padding="3dp"
                    android:inputType="textMultiLine"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_horizontal"
            android:layout_marginBottom="10dp"
            android:layout_above="@+id/layout_button2">
            <Button
                android:id="@+id/btnClearLog"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Clear\nLog"/>

            <Button
                android:id="@+id/btnStartTrx"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:enabled="false"
                android:text="Start Transaction"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout_button2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_horizontal"
            android:layout_above="@+id/layout_button3">
            <Button
                android:id="@+id/btnVoidTrx"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:enabled="false"
                android:text="Void\nTransaction"/>

            <Button
                android:id="@+id/btnGetTransactionStatus"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:enabled="false"
                android:text="Transaction\nStatus"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/layout_button3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_horizontal"
            android:layout_marginBottom="10dp"
            android:layout_alignParentBottom="true">
            <Button
                android:id="@+id/btnRefreshToken"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:enabled="true"
                android:text="Refresh\nToken"/>

            <Button
                android:id="@+id/btnSettlement"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Perform\nSettlement"
                android:enabled="false"
                android:visibility="visible"/>

            <Button
                android:id="@+id/btnRefundTrx"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Refund\nTransaction"
                android:enabled="false"
                android:visibility="gone"/>

            <Button
                android:id="@+id/btnUploadSignature"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Upload\nSignature"
                android:visibility="gone"
                android:enabled="false"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/activity_layoutSensoryBranding"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:background="@color/colorWhite"
            android:visibility="gone">
            <com.visa.SensoryBrandingView
                android:id="@+id/activity_visaSensoryBrandingView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:visibility="gone"/>

            <VideoView
                android:id="@+id/activity_sensoryBrandingVideoView"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:visibility="gone"/>
        </LinearLayout>
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>